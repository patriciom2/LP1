#include <iostream>

using namespace std;
int main(){
		char i;
	    char k;
	    int g=0;
	
	    for(i='a';i<='z';i++){
		    cout<<i<<"\t"<<g<<endl;
		    g++;
	    }
	    cout<<endl;
	
	    for(k='A';k<='Z';k++){
		    cout<<k<<"\t"<<g<<endl;
		    g++;
	    }
	    cout<<endl;

	    for(int h=0;h<=7;h++){
		    cout<<h<<endl;
	    }
	    cout<<endl;
}

